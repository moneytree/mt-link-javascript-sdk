{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "lib": ["es2015", "dom"]
  },
  "include": ["src/**/*"]
}
